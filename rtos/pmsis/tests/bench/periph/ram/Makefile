RAM_TYPE ?= HYPERRAM

ifeq '$(RAM_TYPE)' 'HYPERRAM'
APP_CFLAGS += -DUSE_HYPERRAM
else
APP_CFLAGS += -DUSE_SPIRAM
CONFIG_SPIRAM = 1
endif

ITF_FREQUENCY ?= 200
FC_FREQUENCY ?= 200

APP_CFLAGS += -DITF_FREQUENCY=$(ITF_FREQUENCY) -DFC_FREQUENCY=$(FC_FREQUENCY)

ifdef FULL_PLOT
APP_CFLAGS += -DFULL_PLOT=1
endif

APP = test
APP_SRCS = test.c
APP_CFLAGS += -O3 -g
APP_LDFLAGS += -O3 -g

include $(RULES_DIR)/pmsis_rules.mk
